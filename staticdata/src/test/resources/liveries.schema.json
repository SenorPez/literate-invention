{
  "$schema": "http://json-schema.org/draft-6/schema",
  "title": "Project CARS Static Data API Car Liveries",
  "description": "JSON Representation of Project CARS Static Data API car liveries list.",
  "definitions": {
    "standardLink": {
      "type": "object",
      "properties": {
        "href": {
          "type": "string",
          "pattern": "^http[s]?://[a-z.]+((/[a-z0-9-]+)+|/)$"
        }
      },
      "additionalProperties": false,
      "required": [
        "href"
      ]
    },
    "curies": {
      "type": "array",
      "items": {
        "properties": {
          "href": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "templated": {
            "type": "boolean"
          }
        },
        "additionalProperties": false,
        "required": [
          "href",
          "name",
          "templated"
        ]
      }
    }
  },
  "type": "object",
  "properties": {
    "_embedded": {
      "type": "object",
      "properties": {
        "pcars:livery": {
          "type": "array",
          "items": {
            "properties": {
              "id": {
                "type": "number"
              },
              "name": {
                "type": "string"
              },
              "_links": {
                "type": "object",
                "properties": {
                  "self": {
                    "oneOf": [
                      {
                        "type": "array",
                        "items": {
                          "$ref": "#/definitions/standardLink"
                        }
                      },
                      {
                        "$ref": "#/definitions/standardLink"
                      }
                    ]
                  }
                },
                "additionalProperties": false,
                "required": [
                  "self"
                ]
              }
            },
            "additionalProperties": false,
            "required": [
              "id",
              "name",
              "_links"
            ]
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "pcars:livery"
      ]
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/standardLink"
              }
            },
            {
              "$ref": "#/definitions/standardLink"
            }
          ]
        },
        "index": {
          "$ref": "#/definitions/standardLink"
        },
        "curies": {
          "$ref": "#/definitions/curies"
        },
        "pcars:car": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/standardLink"
              }
            },
            {
              "$ref": "#/definitions/standardLink"
            }
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "self",
        "index",
        "curies",
        "pcars:car"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "_embedded",
    "_links"
  ]
}